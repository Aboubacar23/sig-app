<form method="post" action="{{ path('app_banque_delete', {'id': banque.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ banque.id) }}">
    <button class="btn btn-danger btn-sm">Supprimer</button>
</form>
