 {% extends 'base_print.html.twig' %}

{% block title %}facture index{% endblock %}

{% block body %}
<div class="header">
<div class="header_left">
        <img src="data:image/jpeg;base64,
" width="100" height="100">
 </div>
<div class="header_right">
    <h2 class="text-primary">Facture Client</h1>
    <p><span class="span_p">Numro Facture :</span> {{ facture_client.numeroFacture}}</p>
    <p><span class="span_p">Numro Commande :</span> {{ facture_client.commande.codeCommande}}</p>
    <p><span class="span_p">Date Facture :</span> {{ facture_client.dateFacturation ? facture_client.dateFacturation|date('d-m-Y') : '' }}</p>
    <p>
        <span class="span_p">Paiement : </span>
        {% if facture_client.paiement == true %}
            <span style="color: green;">{{ facture_client.paiement ? 'Payer' : 'Non Payer' }}</span>
        {% else %}
            <span style="color: red;">{{ facture_client.paiement ? 'Payer' : 'Non Payer' }}</span>
        {% endif %}
    </p>
</div>
</div> 
<div class="main_tableau">  
    <div class="tab_left">
        <table>
            <tr id="titre_table">
                <th colspan="2">Client</th>
            </tr>
            <tr>
                <th class="tab_t2">Nom Prenom</th>
                <td>{{ facture_client.commande.client.nom }} {{ facture_client.commande.client.prenom }}</td>
            </tr>
            <tr>
                <th class="tab_t2">Tlephone</th>
                <td>{{ facture_client.commande.client.telephone }}</td>
            </tr>
            <tr>
                <th class="tab_t2">Adresse</th>
                <td>{{ facture_client.commande.client.adresse }}</td>
            </tr>
        </table>
    </div>
    <div class="tab_right">
        <table class="table_tab_right">
            <tr id="titre_table">
                <th colspan="3">Emetteur</th>
            </tr>
            <tr>
                <th class="tab_t2">Entreprise</th>
                <td>SM TRANSIT-STOCK</td>
            </tr>
            <tr>
                <th class="tab_t2">Tlephone</th>
                <td>00 224 622527555</td>
            </tr>
            <tr>
                <th class="tab_t2">Ville, Adresse</th>
                <td>Conakry, Madina</td>
            </tr>
        </table>
    </div>
</div>
<div class="main">
    <div class="tableau">
        <table>
            <thead>
                <tr id="titre_table2">
                    <th class="num_tab">N</th>
                    <th class="tab_produit">Produit</th>
                    <th class="tab_q">Qte</th>
                    <th class="tab_prix">Prix Unitaire (GNF)</th>
                    <th class="tab_prix">Remise (GNF)</th>
                    <th class="tab_q">TVA%</th>
                    <th class="tab_prix">Total HT (GNF)</th>
                    <th class="tab_prix">Total TTC (GNF)</th>
                </tr>
            </thead>
            <tbody>   
                {%  set montantTotalTTC = 0 %}
                {%  set montantHT = 0 %}
                {%  set montantTTC = 0 %}
                {%  set montantTVA = 0 %}
                {%  set montantNet = 0 %}

                {%  set i = 0 %}
            {% for l in lcommandes %}
                {% if l.numCommande == facture_client.commande.codeCommande %}
                    {%  set i = i + 1 %} 
                    {%  set montantHT = l.quantite * l.produit.prixVente %}
                    {%  set montantTVA = montantHT * (l.tva/100) %}
                    {%  set montantTTC = montantHT + montantTVA-l.remise %}


                    {%  set montantTotalTTC = montantTotalTTC + montantTTC %}
                    {%  set montantNet = montantTotalTTC-facture_client.remise-facture_client.avance %}
                    <tr>
                        <td width="10px">{{ i }}</td>
                        <td>{{l.produit.libelle}}</td>
                        <td>{{l.quantite}}</td>
                        <td class="table_mt">{{l.produit.prixVente | number_format }}</td>
                        <td class="table_mt">{{l.remise}}</td>
                        <td class="table_mt">{{l.tva}}</td>
                        <td class="table_mt">{{ montantHT | number_format }}</td>
                        <td class="table_mt">{{ montantTTC | number_format }}</td>
                    </tr>
                {% endif %} 
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
<div class="total">
    <table>
        <tr>
            <td class="span_p tab_prix">Total</td>
            <td class="tab_right tab_prix">{{montantTotalTTC | number_format}}</td>
        </tr>
        <tr>
            <td class="span_p tab_prix">Remise</td>
            <td class="tab_right tab_prix">{{facture_client.remise | number_format}}</td>
        </tr>
        <tr>
            <td class="span_p tab_prix">Avance</td>
            <td class="tab_right tab_prix">{{facture_client.avance | number_format}}</td>   
        </tr>
        <tr>
            <td class="span_p tab_prix">Total NET</td>
            <td class="tab_right tab_prix">{{montantNet | number_format}}</td>
        </tr>
    </table>
</div>
<div class="footer">
    <div class="footer_left">
        <p><span class="span_p">Mode de paiement: </span>  {{ facture_client.modePaiement}}</p>
        <p><span class="span_p">Condition de paiement: </span>  {{ facture_client.conditionPaiement}}</p>
        <p><span class="span_p">Date de Paiement : </span>  {{ facture_client.commande.paiementDate ? facture_client.commande.paiementDate|date('d-m-Y') : '' }}</p>
        <p><span class="span_p">Date de Reception : </span> {{ facture_client.commande.receptionDate ? facture_client.commande.receptionDate|date('d-m-Y') : '' }}</p>
    </div>
</div>
<div class="footer_date">
    <p>Date :  {{ "now"|date("d-m-Y") }}</p>
</div>
{% endblock %}
