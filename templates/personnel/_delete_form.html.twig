<form method="post" action="{{ path('app_personnel_delete', {'id': personnel.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ personnel.id) }}">
    <button class="btn btn-danger btn-sm">Supprimer <i class="fa fa-trash-o p-1 fa-2x"></i></button>
</form>
